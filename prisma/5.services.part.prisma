model Services {
    id           String @id @default(auto()) @map("_id") @db.ObjectId
    user         String @db.ObjectId
    ServicesName String
    Description  String
    Color        String
    Photo        String?
}

model Term {
    id         String  @id @default(auto()) @map("_id") @db.ObjectId
    ContractId String  @db.ObjectId
    ServiceId  String  @db.ObjectId
    Radio      Boolean
}

model AvailableDays {
    id            String  @id @default(auto()) @map("_id") @db.ObjectId
    ServiceId     String  @db.ObjectId
    Days          Json?
    From          String
    To            String
    rangeRadio    Boolean
    rangeTime     String?
    Recurring     Boolean
    RecurringTime String?
}

model Location {
    id                String @id @default(auto()) @map("_id") @db.ObjectId
    ServiceId         String @db.ObjectId
    arrayOfObjCountry Json?
}

model Coustmer_details {
    id            String @id @default(auto()) @map("_id") @db.ObjectId
    ServiceId     String @db.ObjectId
    detailsTitle  String
    Checkbox      Boolean
    Question_type String
}

model QuestionSchema {
    id                  String  @id @default(auto()) @map("_id") @db.ObjectId
    ServiceId           String  @db.ObjectId
    questionText        String
    question_select     String
   
}

model assignTo {
    id        String @id @default(auto()) @map("_id") @db.ObjectId
    ServiceId String @db.ObjectId
    Assign    Json?
}

model Estimate {
    id                   String  @id @default(auto()) @map("_id") @db.ObjectId
    ServiceId            String  @db.ObjectId
    Customer_will_select String
    Services             Json?
    Discount             String
    Tax                  String
    Recurring            String
    Discount_per_time    String
}


